/*================ Buttons ================*/
.button__btn {
  color: #fff;
  width: 193px;
  height: 38px;
  border: 0;
  text-transform: uppercase;
  font-size: 15px;
  font-weight: bolder;
}

.button__btn--primary {
  background-color: {{ settings.color_primary }};
}

.button__btn--secondary {
  background-color: {{ settings.color_secondary }};
}

.h-100 {
  height: 100%;
}

.v-center {
  position: relative;
  top: 50%;
}